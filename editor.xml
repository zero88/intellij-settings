<application>
  <component name="EditorSettings">
    <option name="languageBreadcrumbsMap">
      <map>
        <entry key="AsciiDoc" value="true" />
        <entry key="CSS" value="true" />
        <entry key="FTL" value="true" />
        <entry key="Groovy" value="true" />
        <entry key="HTML" value="true" />
        <entry key="Haml" value="true" />
        <entry key="JAVA" value="true" />
        <entry key="JSON" value="true" />
        <entry key="JSP" value="true" />
        <entry key="JSPX" value="true" />
        <entry key="Jade" value="true" />
        <entry key="JavaScript" value="true" />
        <entry key="LESS" value="true" />
        <entry key="Markdown" value="true" />
        <entry key="Python" value="true" />
        <entry key="SASS" value="true" />
        <entry key="SCSS" value="true" />
        <entry key="SQL" value="true" />
        <entry key="Scala" value="true" />
        <entry key="Stylus" value="true" />
        <entry key="TypeScript" value="true" />
        <entry key="VTL" value="true" />
        <entry key="XHTML" value="true" />
        <entry key="XML" value="true" />
        <entry key="go" value="true" />
        <entry key="kotlin" value="true" />
        <entry key="protobuf" value="true" />
        <entry key="yaml" value="true" />
      </map>
    </option>
    <option name="USE_SOFT_WRAPS" value="MAIN_EDITOR:PREVIEW" />
    <option name="SOFT_WRAP_FILE_MASKS" value="*.md; *.txt; *.rst; *.adoc" />
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
    <option name="SHOW_INSPECTION_WIDGET" value="false" />
    <option name="SHOW_BREADCRUMBS_ABOVE" value="true" />
    <option name="IS_WHEEL_FONTCHANGE_ENABLED" value="true" />
  </component>
  <component name="InlayHintsSettings">
    <option name="disabledHintProviderIds">
      <set>
        <option value="JAVA.chain.hints" />
      </set>
    </option>
    <option name="lastViewedProviderKeyId" value="docker.inlay.hints" />
  </component>
  <component name="JetEditorOptions">
    <option name="donTShowConversionDialog" value="true" />
  </component>
  <component name="LiveEditOptions" chromeAutoDelay="300" chromeUpdateJSOnChanges="true" restartIfHotSwapFailed="true" updateFilesToCompile="true" />
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <filter name="FIXME">
      <pattern index="1" />
    </filter>
  </component>
  <component name="YamlEditorOptions">
    <option name="useSmartPaste" value="false" />
  </component>
</application>